<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="572.0" prefWidth="916.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.main.SignUpController">
   <children>
      <TextField fx:id="inputId" layoutX="333.0" layoutY="140.0" onKeyReleased="#isIdChk" prefHeight="18.0" prefWidth="250.0" />
      <PasswordField fx:id="inputPw" layoutX="333.0" layoutY="221.0" prefHeight="24.0" prefWidth="250.0" onKeyReleased="#isPwChk" />
      <PasswordField fx:id="inputRepw" layoutX="333.0" layoutY="301.0" onKeyReleased="#isPwChk" prefHeight="24.0" prefWidth="250.0" />
      <TextField fx:id="inputName" layoutX="333.0" layoutY="386.0" prefHeight="18.0" prefWidth="250.0" />
      <Button fx:id="signBtn" layoutX="333.0" layoutY="440.0" mnemonicParsing="false" onAction="#doSign" prefHeight="24.0" prefWidth="250.0" text="회원가입" />
      <Label fx:id="idLabel" layoutX="333.0" layoutY="112.0" prefHeight="24.0" prefWidth="77.0" text="아이디" />
      <Label fx:id="pwLabel" layoutX="333.0" layoutY="197.0" prefHeight="24.0" prefWidth="77.0" text="비밀번호" />
      <Label fx:id="repwLabel" layoutX="333.0" layoutY="278.0" prefHeight="24.0" prefWidth="77.0" text="비밀번호 확인" />
      <Label fx:id="nameLabel" layoutX="333.0" layoutY="362.0" prefHeight="24.0" prefWidth="77.0" text="이름" />
      <Label fx:id="idCheck" layoutX="333.0" layoutY="164.0" prefHeight="24.0" prefWidth="343.0" text="아이디는 영어와 숫자로 이루어진 7~12글자로 입력해주세요." textFill="RED" />
      <Label fx:id="pwRegret" layoutX="333.0" layoutY="245.0" prefHeight="24.0" prefWidth="250.0" />
      <Label fx:id="pwCheck" layoutX="333.0" layoutY="325.0" prefHeight="24.0" prefWidth="250.0" text="비밀번호가 일치하지 않습니다." textFill="RED" />
      <Button fx:id="goMain" layoutX="32.0" layoutY="25.0" mnemonicParsing="false" onAction="#goMainStage" prefHeight="30.0" prefWidth="32.0" text="&lt;-" />
   </children>
</AnchorPane>
