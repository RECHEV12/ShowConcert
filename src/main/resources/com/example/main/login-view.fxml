<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="572.0" prefWidth="916.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.main.LoginController">
   <children>
      <TextField fx:id="inputId" layoutX="351.0" layoutY="203.0" prefWidth="214.0" promptText="abc123" />
      <PasswordField fx:id="inputPw" layoutX="351.0" layoutY="286.0" prefHeight="24.0" prefWidth="214.0" />
      <Label fx:id="idLabel" layoutX="352.0" layoutY="172.0" prefHeight="31.0" prefWidth="65.0" text="아이디" />
      <Label fx:id="pwLabel" layoutX="352.0" layoutY="255.0" prefHeight="31.0" prefWidth="65.0" text="비밀번호" />
      <Button fx:id="submitLogin" layoutX="352.0" layoutY="416.0" mnemonicParsing="false" onAction="#doLogin" prefHeight="24.0" prefWidth="214.0" text="로그인" />
      <Label fx:id="createId" layoutX="371.0" layoutY="379.0" prefHeight="16.0" prefWidth="176.0" text="아이디가 없으신가요? 회원가입" onMouseClicked="#goSignup"/>
      <Label fx:id="findId" layoutX="398.0" layoutY="343.0" prefHeight="16.0" prefWidth="120.0" text="아이디/비밀번호 찾기"  onMouseClicked="#goFindPw"/>
      <Button fx:id="goMainStage" layoutX="25.0" layoutY="23.0" mnemonicParsing="false" onAction="#goMainStage" prefHeight="31.0" prefWidth="41.0" text="&lt;-" />
   </children>
</AnchorPane>
